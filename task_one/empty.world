<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="empty">
    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>

    <include>
      <uri>model://land_pad</uri>
      <pose>0 0 0.1 0 0 0</pose>
    </include>

    <!-- Green grass ground plane -->
    <model name="grass_field">
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>120 53.3</size>
            </plane>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>100</mu>
                <mu2>50</mu2>
              </ode>
            </friction>
          </surface>
        </collision>
        <visual name="visual">
          <cast_shadows>false</cast_shadows>
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>120 53.3</size>
            </plane>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Grass</name>
            </script>
          </material>
        </visual>
      </link>
    </model>

    <!-- Individual yard lines -->
    <model name="yard_lines">
      <static>true</static>
      <link name="link">
        <!-- Sidelines -->
        <visual name="sideline_1">
          <pose>0 26.65 0.01 0 0 0</pose>
          <geometry>
            <box>
              <size>120 0.2 0.01</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/White</name>
            </script>
          </material>
        </visual>
        <visual name="sideline_2">
          <pose>0 -26.65 0.01 0 0 0</pose>
          <geometry>
            <box>
              <size>120 0.2 0.01</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/White</name>
            </script>
          </material>
        </visual>

        <!-- 10-yard increments -->
        <visual name="yard_0">
          <pose>-60 0 0.01 0 0 0</pose>
          <geometry>
            <box>
              <size>0.2 53.3 0.01</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/White</name>
            </script>
          </material>
        </visual>
        <visual name="yard_10">
          <pose>-40 0 0.01 0 0 0</pose>
