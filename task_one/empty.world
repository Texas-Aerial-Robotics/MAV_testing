        </visual>
        <visual name="yard_20">
          <pose>-20 0 0.01 0 0 0</pose>
          <geometry>
            <box>
              <size>0.2 53.3 0.01</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/White</name>
            </script>
          </material>
        </visual>
        <visual name="yard_30">
          <pose>0 0 0.01 0 0 0</pose>
          <geometry>
            <box>
              <size>0.2 53.3 0.01</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/White</name>
            </script>
          </material>
        </visual>
        <visual name="yard_40">
          <pose>20 0 0.01 0 0 0</pose>
          <geometry>
            <box>
              <size>0.2 53.3 0.01</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/White</name>
            </script>
          </material>
        </visual>
        <visual name="yard_50">
          <pose>40 0 0.01 0 0 0</pose>
          <geometry>
            <box>
              <size>0.2 53.3 0.01</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/White</name>
            </script>
